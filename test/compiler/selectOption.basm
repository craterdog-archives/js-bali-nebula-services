
1.SelectStatement:
LOAD LITERAL `$selection`
STORE VARIABLE $_selector_1

1.1.OptionClause:
LOAD VARIABLE $_selector_1
LOAD LITERAL `'alpha'`
INVOKE INTRINSIC $matches WITH 2 PARAMETERS
JUMP TO 1.2.OptionClause ON FALSE

1.1.1.EvaluateStatement:
LOAD LITERAL `none`
STORE VARIABLE $_result_

1.1.OptionDone:
JUMP TO 1.StatementDone

1.2.OptionClause:
LOAD VARIABLE $_selector_1
LOAD LITERAL `'beta'`
INVOKE INTRINSIC $matches WITH 2 PARAMETERS
JUMP TO 1.3.OptionClause ON FALSE

1.2.1.EvaluateStatement:
LOAD LITERAL `none`
STORE VARIABLE $_result_

1.2.OptionDone:
JUMP TO 1.StatementDone

1.3.OptionClause:
LOAD VARIABLE $_selector_1
LOAD LITERAL `'gamma'`
INVOKE INTRINSIC $matches WITH 2 PARAMETERS
JUMP TO 1.StatementDone ON FALSE

1.3.1.EvaluateStatement:
LOAD LITERAL `none`
STORE VARIABLE $_result_

1.StatementDone:
SKIP INSTRUCTION

