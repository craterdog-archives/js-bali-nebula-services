STORE VARIABLE $_continue_1_Label_

1.Label:
LOAD VARIABLE $_continue_1_Label_
LOAD VARIABLE $condition
INVOKE INTRINSIC $and WITH 2 PARAMETERS
JUMP TO 1.FinishClause ON FALSE

1.1.EvaluateStatement:
LOAD VARIABLE $b
STORE VARIABLE $a

1.2.IfStatement:
SKIP INSTRUCTION

1.2.ConditionClause_1:
LOAD VARIABLE $a
JUMP TO 1.2.FinishClause ON FALSE

1.2.1.BreakStatement:
LOAD LITERAL `false`
STORE VARIABLE $_continue_1_Label_
LOAD LITERAL `true`
STORE VARIABLE $_loop_interrupted_
JUMP TO 1.2.FinishClause

1.2.FinishClause:
LOAD VARIABLE $_exception_thrown_
JUMP TO 1.FinishClause ON TRUE
LOAD VARIABLE $_loop_interrupted_
JUMP TO 1.Label ON TRUE
LOAD VARIABLE $_result_returned_
JUMP TO 1.FinishClause ON TRUE

